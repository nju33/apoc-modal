{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["> 3%", "last 2 versions"],
      },
      "modules": false
    }],
    "flow"
  ],
  "plugins": [
    "transform-runtime",
    "external-helpers",
    "transform-object-rest-spread",
    "transform-class-properties"
  ],
  "env": {
    "test": {
      "presets": [
        ["env", {
          "targets": {
            "browsers": ["> 3%", "last 2 versions"]
          }
        }],
        "flow"
      ],
      "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties"
      ],
    }
  }
}
